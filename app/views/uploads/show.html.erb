<p id="notice"><%= notice %></p>

  <% @upload.files.each do |file| %>

  <table>
    <tr>
      <td><%= file.name %></td>
      <td><%= l file.created_at, format: :short %></td>
      <td><%= file.id %></td>
    </tr>
  </table>

<% end %>
<p>
  <strong>File name:</strong>
  <%= @upload.file_name %>
</p>

<p>
  <strong>File ext:</strong>
  <%= @upload.file_ext %>
</p>

<p>
  <strong>File size:</strong>
  <%= @upload.file_size %>
</p>

<p>
  <strong>Game version:</strong>
  <%= @upload.game_version %>
</p>

<p>
  <strong>Project:</strong>
  <%= @upload.project_id %>
</p>

<%= link_to 'Edit', edit_upload_path(@upload) %> |
<%= link_to 'Back', uploads_path %>
